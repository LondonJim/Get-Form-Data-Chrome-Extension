{
  "manifest_version": 2,

  "name": "Form Data",
  "description": "Looks at some form data.",
  "version": "0.1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
                  "webRequest",
                  "https://www.ubereats.com/*",
                  "https://www.facebook.com/*",
                  "tabs"
                 ],

  "browser_action": {
    "default_popup": "./popup/index.html"
  }
}
